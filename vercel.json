{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/((?!api/.*).*)",
      "destination": "./apps/dist/index.html"
    },
    {
      "source": "/api/:path*",
      "destination": "./apps/api/src/main.ts"
    }
  ]
}
